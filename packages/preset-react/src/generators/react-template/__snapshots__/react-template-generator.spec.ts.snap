                  "return (",
                  "<AppModal",
                  "hide={hide}",
                  "show={show}",
                  "title="Airdrop"",
                  "submitDisabled={!amount || mutation.isPending}",
                  "submitLabel="Request Airdrop"",
                  "submit={() => mutation.mutateAsync(parseFloat(amount)).then(() => hide())}",
