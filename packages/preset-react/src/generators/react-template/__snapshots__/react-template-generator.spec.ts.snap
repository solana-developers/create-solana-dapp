                  "<input",
                  "disabled={mutation.isPending}",
                  "type="number"",
                  "step="any"",
                  "min="1"",
                  "placeholder="Amount"",
                  "className="input input-bordered w-full"",
                  "value={amount}",
                  "onChange={(e) => setAmount(e.target.value)}",
