                  "}",
                  "export function AppModal({",
                  "children,",
                  "title,",
                  "hide,",
                  "show,",
                  "submit,",
                  "submitDisabled,",
                  "submitLabel,",
                  "}: {",
                  "children: ReactNode",
                  "title: string",
