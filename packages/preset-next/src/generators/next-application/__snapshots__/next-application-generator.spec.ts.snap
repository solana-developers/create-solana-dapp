                  "useMyProgramProgram,",
                  "useMyProgramProgramAccount,",
                  "} from './my-program-data-access';",
                  "export function MyProgramCreate() {",
                  "const { initialize } = useMyProgramProgram();",
                  "return (",
                  "<button",
                  "className="btn btn-xs lg:btn-md btn-primary"",
